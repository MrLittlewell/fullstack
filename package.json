{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "first app",
  "main": "index.js",
  "engines": {
    "node": "11.2.0",
    "npm":"6.4.1"
  },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm rud build --prefix client"
  },
